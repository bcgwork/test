<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xsi:schemaLocation="
		http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc.xsd
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">


	<mvc:view-controller path="/h5/user/gotoRegister.do"
		view-name="user/register" />

	<mvc:view-controller path="/h5/user/gotoForgotPasswd.do"
		view-name="user/forgot_passwd_step1" />
	<mvc:view-controller path="/h5/user/gotoForgotPasswd_next.do"
		view-name="user/forgot_passwd_step2" />
	<mvc:view-controller path="/404.do"
		view-name="404" />
	<mvc:view-controller path="/h5/user/register_protocol.do"
		view-name="user/register_protocol" />
	<mvc:view-controller path="/h5/news/new_list.do"
		view-name="news/new_list" />

</beans>
